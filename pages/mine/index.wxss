page,page>view{
  height: 100%;
  background: #f3f3f3;
}
.info{
  width: 100%;
  height: 366rpx;
  position: relative;
}
.infoBackgroundImg{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.infoContent{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
}
.headPortrait{
  height: 200rpx;
  width: 100%;
  padding: 45rpx;
  box-sizing: border-box;
}
.headPortrait>image{
  width: 116rpx;
  height: 116rpx;
  border-radius: 100%;
  display: block;
  margin: 0 auto;
}
.headPortrait>text{
  width: 100%;
  text-align: center;
  display: block;
  color: white;
  margin-top: 20rpx;
}
.integral{
  width: 682rpx;
  height: 112rpx;
  margin: 0 auto;
  background: #22120a;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  overflow: hidden;
}
.integralNum{
  float: left;
  width: 49.5%;
  height: 100%;
  text-align: center;
  color: white;
  font-size: 24rpx;
}
.integralNum>text:nth-child(1){
  height: 52rpx;
  display: block;
  font-size: 46rpx;
  color: #fec08b;
  margin: 10rpx 0; 
  text-align: center;
  line-height: 70rpx;
}
.middle{
  float: left;
  width: 1%;
  height: 100%;
  position: relative;
}
.middle::before{
  content: '';
  width: 2rpx;
  height: 60rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  display: block;
  background: #ffe8d4;
}
.signIn{
  float: left;
  width: 49.5%;
  height: 100%;
  text-align: center;
  color: white;
  font-size: 24rpx;
}
.signIn>image{
  width: 52rpx;
  height: 52rpx;
  display: block;
  margin: 10rpx auto;
  border-radius: 52rpx;
}
.mineContent{
  width: 100%;
  /* height: calc(100% - 366rpx); */
  padding: 34rpx;
  box-sizing: border-box;
  background: #f3f3f3;
}
.mineContents{
  width: 100%;
  margin-top: 30rpx;
  border: 1rpx solid #dcdcdc;
  background: white;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.mineContents view:nth-last-child(1){
  border-bottom: 0;
}
.mineContentsTitle{
  width: 100%;
  height: 86rpx;
  border-bottom: 2rpx solid #dcdcdc;
  overflow: hidden;
  padding: 25rpx;
  box-sizing: border-box;
}
.mineContentsTitle>view:nth-child(1){
  float: left;
  font-size: 30rpx;
}
.mineContentsTitle>view:nth-child(2){
  float: right;
  font-size: 26rpx;
  color: #696969;
}
.mineContentsText{
  width: 100%;
  height: 164rpx;
  display: flex;
  justify-content: space-around;
}
.mineContentsText>view{
  float: left;
  width: 80rpx;
  font-size: 24rpx;
  text-align: center;
  padding-top: 30rpx;
  box-sizing: border-box;
  position: relative;
}
.mineContentsText>view>image{
  width: 58rpx;
  height: 58rpx;
  margin: 0 auto 16rpx;
  display: block;
}
.mineContentKefu{
  padding: 0 24rpx;
  box-sizing: border-box;
}
.mineContentKefu>view{
  width: 100%;
  border-bottom: 2rpx solid #dcdcdc;
  padding: 30rpx 0;
  overflow: hidden;
}
.mineContentKefu>view>text{
  float: left;
  margin-left: 20rpx;
  font-size: 28rpx;
  font-weight: 300;
}
.mineContentKefu>view>image{
  width: 24rpx;
  height: 32rpx;
  float: right;
  margin-right: 30rpx;
}
.notLoggedIn{
  width: 100%;
  overflow: hidden;
  padding: 46rpx;
  box-sizing: border-box;
}
.notLoggedIn>image{
  width: 116rpx;
  height: 116rpx;
  border-bottom: 1rpx solid #fa9e44;
  box-sizing: border-box;
  border-radius: 100%;
  border: 1rpx solid #fec08b;
  display: block;
  float: left;
}
.notLoggedIn>view{
  width: calc(100% - 116rpx);
  height: 116rpx;
  float: left;
  padding-left: 44rpx;
  box-sizing: border-box;
  line-height: 116rpx;
  position: relative;
}
.notLoggedIn>view>text{
  float: left;
  color: white;
  font-size: 32rpx;
}
.notLoggedIn>view>image{
  width: 24rpx;
  height: 32rpx;
  /* margin-right: 30rpx; */
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
}
.reddot{
  background: #e1360c;
  height: 34rpx;
  width: 34rpx;
  border-radius: 34rpx;
  position: absolute;
  right: 6rpx;
  top: 20rpx;
  color: white;
  font-size: 20rpx;
  line-height: 34rpx;
  text-align: center;
}



.popups{
  width: 100%;
  height: calc(100% );
  background: rgba(0, 0, 0, .8);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
}

.popUpsContent{
  width: 550rpx;
  margin: 20rpx auto 0;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
}
.popUpsContentText{
  width: 550rpx;
  height: 654rpx;
  background: white;
  border-radius: 10rpx;
  box-sizing: border-box;
  position: relative;
}
.popUpsContentText>view:nth-child(1){
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: white;
  padding-top: 100rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 4;
}
.popUpsContentTextImg{
  width: 208rpx;
  height: 208rpx;
  margin: 56rpx auto 18rpx;
  position: relative;
  z-index: 4;
}
.popUpsContentTextImg image{
  width: 100%;
  height: 100%;
}
.popUpsContentText>view:nth-child(3){
  width: 270rpx;
  margin: 0 auto;
  /* line-height: 44rpx; */
  font-size: 24rpx;
}
.popUpsContentText>view:nth-child(3) view{
  margin-bottom: 24rpx;
}
.popUpsBtn{
  width: 84rpx;
  height: 84rpx;
  border-radius: 84rpx;
  margin:  50rpx auto 0;
}
.popUpsBtn>image{
  width: 100%;
  height: 100%;
}
.cs{
  width: 100%;
  height: 10rpx;
  background: #f3f3f3;
}
.css{
  width: 0;
  height: 100%;
  background: red;
}
.popUpsContentTextForm{
  width: 100%;
}
.popUpsContentTextForm>view{
  width: 100%;
  overflow: hidden;
  height: 94rpx;
  border-bottom: 2rpx solid #ccc;
  padding-left: 40rpx;
  box-sizing: border-box;
  line-height: 94rpx;
  position: relative;
}
.popUpsContentTextForm>view>text{
  float: left;
  text-align: center;
}
.popUpsContentTextForm>view>input{
  border: none;
  float: left;
  position: absolute;
  right: -5rpx;
  top: 50%;
  transform: translateY(-50%);
}
.popImgs{
  width: 100%;
  height: 272rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
}