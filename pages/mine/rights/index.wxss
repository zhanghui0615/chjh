page,page>view{
  height: 100%;
}
.rightsTitle{
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
}
.rightsContent{
  width: 100%;
}
.rightsMin{
  width: 100%;
  height: 460rpx;
  /* padding: 28rpx; */
  box-sizing: border-box;
  background: #f3f3f3;
}
.rightsMinDom{
  width: 100%;
  height: 100%;
  /* border-radius: 20rpx; */
  background: #201008;
  /* padding: 26rpx 0rpx 26rpx 26rpx; */
  box-sizing: border-box;
  position: relative;
}
.backgrounImg{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.backgrounImg image{
  width: 300rpx;
  height: 100%;
}
.backgrounImg image:nth-child(1){
  position: absolute;
  right: 0;
  top: 0;
}
.backgrounImg image:nth-child(2){
  position: absolute;
  left: 0;
  bottom: 0;
}
.backgrounText {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
}
.rightsMinDomTitle{
  font-size: 30rpx;
  color: #f0b365;
  overflow: hidden;
  padding: 26rpx;
  box-sizing: border-box;
}
.rightsMinDomTitle view:nth-child(1){
  float: left;
}
.rightsMinDomTitle view:nth-child(2){
  float: right;
  font-size: 28rpx;
}
.rightsMinDomContent{
  padding:0 26rpx 26rpx 26rpx;
  height: 220rpx;
  box-sizing: border-box;
}
.rightsMinDomContentView{
  width: 260rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  color: white;
  border-radius: 35rpx;
  font-size: 28rpx;
  border: 1rpx solid #f0b365;
  margin: 30rpx auto 24rpx;
}
.increment{
  width: 100%;
}
.incrementTitle{
  width: 100%;
  padding: 32rpx 0;
  border-top:  1rpx solid #ccc;
  border-bottom:  1rpx solid #ccc;
}
.incrementTitle view{
  width: 100%; 
  font-size: 32rpx;
  border-left: 12rpx solid #e1360c;
  padding-left: 34rpx;
  box-sizing: border-box;
}
.incrementContent{
  width: 100%;
}
.tabNav{
  width: 100%;
  height: 88rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.tabNav>view{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.tabNav>view>view{
  width: 33.333333%;
  height: 100%;
  text-align: center;
  line-height: 88rpx;
  font-size: 30rpx;
  float: left;
}
.clickViewBackground{
  background: #f58b44;
  color: white;
}
.tabContent{
  width: 100%;
  background: #f58b44;
  position: relative;
}
.tabContent>view{
  width: 100%;
  height: 1500rpx;
  /* margin-top: 60rpx; */
  color:white;
  font-size: 28rpx;
  position: relative;
  z-index: 3;
}
.imagese{
  width: 400rpx;
  height: 90rpx;
  position: absolute;
  right: -50rpx;
  top: -12rpx;
}
.imgs{
  left: calc(50% - 400rpx);
  transform: translateX(calc(-50% + 400rpx))
}
.imgs1{
  left: -50rpx;
}
.imse{
  width: 400rpx;
  height: 90rpx;
  position: absolute;
  right: -50rpx;
  top: -12rpx;
  z-index: 10;
  line-height: 90rpx;
  text-align: center;
}
.tabContent>view>view{
  width: 100%;
  padding: 30rpx 76rpx;
  box-sizing: border-box;
  overflow: hidden; text-overflow:ellipsis; white-space: nowrap;
}
.popups{
  width: 100%;
  height: calc(100% );
  background: rgba(0, 0, 0, .8);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
}

.popUpsContent{
  width: 550rpx;
  margin: 20rpx auto 0;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
}
.popUpsContentText{
  width: 550rpx;
  height: 654rpx;
  background: white;
  border-radius: 10rpx;
  box-sizing: border-box;
  position: relative;
}
.popUpsContentText>view:nth-child(1){
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: white;
  padding-top: 100rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 4;
}
.popUpsContentTextImg{
  width: 208rpx;
  height: 208rpx;
  margin: 56rpx auto 18rpx;
  position: relative;
  z-index: 4;
}
.popUpsContentTextImg image{
  width: 100%;
  height: 100%;
}
.popUpsContentText>view:nth-child(3){
  width: 270rpx;
  margin: 0 auto;
  /* line-height: 44rpx; */
  font-size: 24rpx;
}
.popUpsContentText>view:nth-child(3) view{
  margin-bottom: 24rpx;
}
.popUpsBtn{
  width: 84rpx;
  height: 84rpx;
  border-radius: 84rpx;
  margin:  50rpx auto 0;
}
.popUpsBtn>image{
  width: 100%;
  height: 100%;
}
.cs{
  width: 100%;
  height: 10rpx;
  background: #f3f3f3;
}
.css{
  width: 0;
  height: 100%;
  background: red;
}
.popUpsContentTextForm{
  width: 100%;
}
.popUpsContentTextForm>view{
  width: 100%;
  overflow: hidden;
  height: 94rpx;
  border-bottom: 2rpx solid #ccc;
  padding-left: 40rpx;
  box-sizing: border-box;
  line-height: 94rpx;
  position: relative;
}
.popUpsContentTextForm>view>text{
  float: left;
  text-align: center;
}
.popUpsContentTextForm>view>input{
  border: none;
  float: left;
  position: absolute;
  right: -5rpx;
  top: 50%;
  transform: translateY(-50%);
}

.rightsMinDomContentViews{
  width: 100%;
  white-space:nowrap;
  margin-top: 50rpx;
}
.scroll{
  min-width:100%;
  box-sizing: border-box;
}
.scroll>view:nth-child(1){
  margin-left: 0;
}
.scrollse{
  display: inline-block;
  width: 220rpx;
  height: 220rpx;
  /* background: black; */
  margin-left: 40rpx;
  position: relative;
  font-size: 30rpx;
}
.scrollse:nth-child(1){
  margin-left:0; 
}
.sdsd{
  position: absolute;
  left: 0;
  top: 0;
  width: 220rpx !important;
  height: 220rpx !important;
  z-index: 0;
}
.scrollsetext{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  color: white;
  padding: 20rpx;
  text-align:center;
  box-sizing: border-box;
  font-size: 30rpx;
}
.scrollsetext view:nth-child(2){
  position: absolute;
  bottom: 0rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #fad093;
}
.rightsMinDomTag{
  width: 100%;
  height: 53rpx;
  padding: 26rpx;
  box-sizing: border-box;
}
.rightsMinDomTag>view{
  width: 164rpx;
  height: 53rpx;
  font-size: 28rpx;
  background: linear-gradient(to right, #eed2ba, #dcb39e);
  border-radius: 10rpx;
  text-align: center;
  line-height: 53rpx;
  color: #51362f;
  font-weight: 500;
}
.popImgs{
  width: 100%;
  height: 272rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
}
.xinxis{
  width: 100%;
  padding: 15rpx 0;
  box-sizing: border-box;
  background: #f3f3f3;
}
.infos{
  width: 100%;
  height: 200rpx; 
  border-top: 2rpx solid #ccc;
  /* border-bottom: 2rpx solid #ccc; */
  background: white;
  padding: 32rpx 36rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.left{
  float: left;
  width: 120rpx;
  height: 120rpx;
  border-radius: 120rpx;
}
.left image{
  width: 100%;
  height: 100%;
  border-radius: 120rpx;
}
.lefts{
  width: 220rpx;
  margin-left: 30rpx;
  float: left;
}
.lefts>view:nth-child(1){
  font-size: 26rpx;
  margin-bottom: 23rpx;
}
.lefts>view:nth-child(2){
  width: 220rpx;
  font-size: 26rpx;
  text-align: center;
}
.right{
  float: right;
  line-height: 200rpx;
}
.right image{
  width: 44rpx;
  height: 44rpx;
  /* transform: rotate(180deg); */
}
.information{
  width: 100%;
  border-bottom: 2rpx solid #ccc;
  background: white;
}
.informationTexts{
  width: 100%;
  overflow: hidden;
  line-height: 56rpx;
  font-size: 26rpx;
}
.infoLeft{
  float: left;
  width: 170rpx;
  color: #5a5a5a;
  text-align: right;
}
.infoRight{
  float: left;
  color: #000000;
  margin-left: 18rpx;
}
.vipContact{
  width: 100%;
  background: white;
  border-top: 2rpx solid #ccc;
}
.vipContactTitle{
  width: 100%;
  padding: 24rpx 0;
  font-size: 28rpx;
}
.vipContactTitle view{
  padding-left: 34rpx;
  border-left: 12rpx solid #e1360c;
}
.infoContens{
  width: 100%;
}
.ifopsd{
  width: 100%;
  font-size: 26rpx;
  overflow: hidden;
}
.ifopsd>view{
  float: left;
  margin-left: 50rpx;
  line-height: 50rpx;
}
.ifopsd>view>view:nth-child(1){
  color: #5a5a5a;
  float: left;
  text-align: right;
  margin-right: 18rpx;
}
.ifopsd>view>view:nth-child(2){
  float: left;
}
.sdaasimds{
  position: absolute;
  z-index: 2
}
.sadeimg{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
}